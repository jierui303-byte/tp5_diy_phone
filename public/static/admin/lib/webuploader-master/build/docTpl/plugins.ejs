<div>
    <hr />
    <h2 id="<%- gennerateID(module + ':' + name + ':plugins') %>">插件介绍</h2>
    <br />
    <ol class="plugin-list">
    <% _.forEach( plugins, function( item ) { %>
    <li id="<%- gennerateID(module + ':' + item.name) %>">
        <code><%= item.shortname || item.name %></code><%- markdown(item.description) %>
        <%- renderFileInfo( item.file ) %>
    </li>
    <% }) %>
    </ol>
</div>